; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Fish",
"language": "GDScript",
"path": "res://fishies/fishy.gd"
}, {
"base": "Area2D",
"class": "Harpoon",
"language": "GDScript",
"path": "res://player/harpoon/harpoon.gd"
}, {
"base": "Node",
"class": "SceneLoader",
"language": "GDScript",
"path": "res://components/scene_loader/scene_loader.gd"
}, {
"base": "Area2D",
"class": "Urchin",
"language": "GDScript",
"path": "res://fishies/urchin/urchin.gd"
} ]
_global_script_class_icons={
"Fish": "",
"Harpoon": "",
"SceneLoader": "",
"Urchin": ""
}

[application]

config/name="GWJ41"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[autoload]

Game="*res://game/game.tscn"

[display]

window/size/width=256
window/size/height=200
window/size/test_width=1024
window/size/test_height=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[layer_names]

2d_physics/layer_1="harpoon"
2d_physics/layer_2="fishies"
2d_physics/layer_3="walls"
2d_physics/layer_4="fish hitbox"
2d_physics/layer_5="player"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.054902, 0.270588, 0.321569, 1 )
environment/default_environment="res://default_env.tres"
